multipass: true

plugins:
    - removeMetadata 
    - removeComments
    - removeTitle
    - removeXMLProcInst
    - removeUselessDefs
    - removeEditorsNSData
    - removeEmptyAttrs
    - removeEmptyText
    - removeEmptyContainers
    - removeUselessStrokeAndFill
    - removeUnusedNS
    - removeDimensions
    - removeViewBox
    - removeUnknownsAndDefaults
    - removeAttrs:
        attrs: "font-family"
    - cleanupListOfValues    
    - collapseGroups        
    - cleanupIDs
    - cleanupAttrs   
    - cleanupNumericValues
    - moveElemsAttrsToGroup
    - moveGroupAttrsToElems 
    - minifyStyles
